{
  "name": "berkut",
  "version": "1.0.0",
  "description": "BERKUT VJ Software",
  "main": "index.js",
  "scripts": {
    "bootstrap": "npm install && bower install && gulp deploy_bower",
    "scss-lint": "sass-lint -c .sass-lint.yml 'app/assets/styles/**/*.scss' -v",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yadex205/berkut.git"
  },
  "author": "Kanon Kakuno <yadex205@outlook.jp>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/yadex205/berkut/issues"
  },
  "homepage": "https://github.com/yadex205/berkut#readme",
  "private": true,
  "config": {
    "target": "1.3.3",
    "arch": "x64",
    "disturl": "https://atom.io/download/atom-shell",
    "runtime": "electron",
    "buildFromSource": true
  },
  "packagingOptions": {
    "arch": "x64",
    "dir": ".",
    "ignore": [
      "bower_components",
      ".gitignore",
      ".eslintrc.json",
      ".sass-lint.yml",
      "gulpfile.js",
      "app",
      "bower.json",
      "README.md",
      "LICENSE",
      "dist"
    ],
    "prune": true,
    "overwrite": true,
    "out": "dist"
  },
  "devDependencies": {
    "electron": "1.3.3",
    "electron-packager": "^7.7.0",
    "graceful-fs": "^4.1.6",
    "gulp": "^3.9.1",
    "gulp-cached": "^1.1.0",
    "gulp-change": "^1.0.0",
    "gulp-ejs": "^2.1.3",
    "gulp-livereload": "^3.8.1",
    "gulp-markdown": "^1.2.0",
    "gulp-notify": "^2.2.0",
    "gulp-plumber": "^1.1.0",
    "gulp-sass": "^2.3.2",
    "gulp-sourcemaps": "^1.6.0",
    "gulp-util": "^3.0.7",
    "lodash": "^4.15.0",
    "minimatch": "^3.0.3",
    "rimraf": "^2.5.4"
  },
  "dependencies": {
    "jquery": "^3.1.0",
    "vue": "^1.0.26"
  }
}
